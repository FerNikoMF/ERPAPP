{
  "name": "erp-app",
  "version": "1.0.0",
  "description": "Electron erp app",
  "author": "FerNikoMF",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "uz.getter.erp.app",
    "publish": [
      {
        "provider": "github",
        "owner": "FerNikoMF",
        "repo": "ERPAPP"
      }
    ],
    "publishAutoUpdate": true
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    }
  },
  "dependencies": {
    "electron-updater": "^5.3.0"
  },
  "devDependencies": {
    "electron": "^29.4.6",
    "electron-builder": "^26.0.0"
  }
}
